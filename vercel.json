{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "talent-profile.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/my-upload-handler.js", "dest": "/api/my-upload-handler.js" },
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/", "dest": "/talent-profile.html" }
  ]
}
